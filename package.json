{
  "name": "@4lch4/benimaru",
  "description": "A CLI utility to help with connecting/managing my tech infrastructure.",
  "version": "0.0.0",
  "author": {
    "name": "4lch4",
    "email": "hey@4lch4.email",
    "url": "https://alcha.org"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "pretty:check": "prettier -c .",
    "pretty": "prettier --write ."
  },
  "bin": {
    "benimaru": "./bin/run"
  },
  "bugs": "https://github.com/4lch4/benimaru/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^4.1.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "ssh2": "^1.5.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/ssh2": "^0.5.48",
    "chai": "^4",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/4lch4/benimaru",
  "keywords": [
    "oclif"
  ],
  "repository": "4lch4/benimaru",
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "benimaru",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-commands",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "types": "dist/index.d.ts"
}
